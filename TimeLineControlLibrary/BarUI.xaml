<UserControl x:Class="TimeLineControlLibrary.BarUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TimeLineControlLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ToolTip x:Key="tooltip" Content="Tooltip"/>
    </UserControl.Resources>
    <Grid>
        <Rectangle x:Name="Body" MouseEnter="Body_MouseEnter" MouseLeave="Body_MouseLeave"></Rectangle>
        
    </Grid>
</UserControl>
